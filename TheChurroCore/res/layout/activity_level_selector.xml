<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainLayout"
    tools:context="${relativePackage}.${activityClass}" >

        <LinearLayout
			android:orientation="vertical"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:layout_gravity="center"
			android:gravity="center">
            
            <LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="0dp"
				android:layout_gravity="center"
				android:gravity="center"
				android:layout_weight="25">
                <ImageButton android:id="@+id/level1"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="fill_parent"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
                <ImageButton android:id="@+id/level2"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="fill_parent"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
                <ImageButton android:id="@+id/level3"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="fill_parent"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
            </LinearLayout>
            <LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="0dp"
				android:layout_gravity="center"
				android:gravity="center"
				android:layout_weight="25">
                <ImageButton android:id="@+id/level4"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="wrap_content"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
                <ImageButton android:id="@+id/level5"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="wrap_content"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
                <ImageButton android:id="@+id/level6"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="wrap_content"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
            </LinearLayout>
            <LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="0dp"
				android:layout_gravity="center"
				android:gravity="center"
				android:layout_weight="25">
                <ImageButton android:id="@+id/level7"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="wrap_content"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
                <ImageButton android:id="@+id/level8"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="wrap_content"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
                <ImageButton android:id="@+id/level9"
                    android:src="@drawable/level1"
	                android:layout_width="0dp"
	                android:layout_height="wrap_content"
	                android:scaleType="centerInside"
	                android:layout_gravity="center"
	                android:background="@null"
	                android:padding="10dp"
	                android:layout_weight="30" />
            </LinearLayout>
            <LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="0dp"
				android:layout_gravity="center"
				android:gravity="center"
				android:layout_weight="15">
                <Button
    	            android:id="@+id/back"
    	            style="@style/smallButton"
    	            android:layout_width="wrap_content"
    	            android:layout_height="wrap_content"
    	            android:text="@string/main_menu"
    	            android:layout_margin="5dp"/>
            </LinearLayout>
            <LinearLayout
	    	    android:id="@+id/anuncio"
			    android:orientation="vertical"
			    android:layout_width="match_parent"
			    android:layout_height="0dp"
			    android:background="@color/mainLayout"
			    android:gravity="bottom"
			    android:layout_gravity="bottom"
			    android:layout_weight="10">
	    	</LinearLayout>
		</LinearLayout>
</RelativeLayout>